	<xs:simpleType name="paymentCodeEnumeration">
		<xs:restriction base="xs:string">
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for salary of employees other than lobbyists who are engaged for 10% or more of their time in one month in activities related to lobbying
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for expenses incurred by a lobbyist and paid directly by the filer, or expenses incurred by the filer for goods or services used by a lobbyist
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for legislative related-services performed by a lobbying firm under Regulation 18614(b)(2)
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for consultants and government relations
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for public affairs, including coalition building and grassroots campaigns
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for advertising
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for research including feasibility studies, analysis, polling and public opinion research
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="V">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for lobbying events including event planning, rentals, equipment, and transportation for members or the public
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="O">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						for all other payments not covered by one of the specific categories
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
