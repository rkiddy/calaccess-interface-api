<?xml version="1.0" encoding="utf-8"?>
<xs:schema version="1.0.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:complexType name="individualType">
		<xs:sequence>
			<xs:element name="FirstName" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LastName" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OccupationandEmployer" minOccurs="0" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="committeeType">
		<xs:sequence>
			<xs:element name="Name" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:choice>
				<xs:annotation>
					<xs:documentation xml:lang="en">
						if the committee does not yet have an id value, provide the treasurer's contact information
					</xs:documentation>
				</xs:annotation>
				<xs:element name="CommitteeId" minOccurs="0" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Treasurer" type="treasurerType" minOccurs="0" maxOccurs="1"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="organizationType">
		<xs:sequence>
			<xs:element name="Name" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="treasurerType">
		<xs:complexContent>
			<xs:extension base="individualType">
				<xs:sequence>
					<xs:element name="address" type="addressType" minOccurs="1" maxOccurs="1"/>
					<xs:element name="phone" type="phoneNumberFormat" minOccurs="0" maxOccurs="1"/>
					<xs:element name="fax" type="phoneNumberFormat" minOccurs="0" maxOccurs="1"/>
					<xs:element name="email" type="emailAddressFormat" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="measureType">
		<xs:sequence>
			<xs:element name="NumberOrLetter" minOccurs="1" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- should this be required? -->
			<xs:element name="Title" minOccurs="0" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Jurisdiction" minOccurs="0" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="electionCycleType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
				Allows tagging of transactions using FEC-style election cycle codes, such as 'G2020'
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"/>
			<xs:pattern value="[PGSRO](197[1-9]|19[8-9]\d|20\d{2})?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="noteType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="4000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="noteListType">
		<xs:sequence>
			<xs:element name="Note" type="noteType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="electionSplitType">
		<xs:sequence>
			<xs:element name="Election" type="electionCycleType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Amount" type="xs:decimal"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="electionSplitListType">
		<xs:sequence>
			<xs:element name="Split" type="electionSplitType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="scheduleDPaymentType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Monetary"/>
			<xs:enumeration value="Nonmonetary"/>
			<xs:enumeration value="IndependentExpenditure"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="headerType">
		<xs:sequence>
			<xs:element name="CARS_Ver" type="xs:string" minOccurs="1" maxOccurs="1" default="1.07">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Provide the version of the XSD file against which this file was validated. This will be used to determine if the XSD file used is still supported.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CARS_Form" type="xs:string" minOccurs="1" maxOccurs="1" fixed="460">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Provide the CAL-ACCESS form being submitted.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Soft_Name" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Provide the Name of the Vendor of the software used to generate this file. Together with the Software Version, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Soft_Ver" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Provide the version of the software used to generate this file. Together with the Software Name, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FilerId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Provide the ID Number assigned to this Recipient Committee. This will be validated against the database to ensure the file is loaded to the correct filer.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name='zipcodeFormat'>
		<xs:restriction base='xs:string'>
			<xs:pattern value='[0-9]{5}(-[0-9]{4})?'/>
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="empty">
		<xs:restriction base="xs:string">
			<xs:maxLength value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name='stateFormat'>
		<xs:restriction base='xs:string'>
			<xs:enumeration value="CA"/>
		</xs:restriction>
	</xs:simpleType>
	<!--  (123) 456-7890 | 123-456-7890 -->
	<xs:simpleType name='phoneNumberFormat'>
		<xs:restriction base='xs:string'>
			<xs:pattern value='((\(\d{3}\) ?)|(\d{3}-))?\d{3}-\d{4}'/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="emailAddressFormat">
		<xs:restriction base="xs:string">
			<xs:pattern value="[^@]+@[^\.]+\..+"/>
			<xs:maxLength value="250"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="phoneNumberFormatEmpty">
		<xs:union memberTypes="phoneNumberFormat empty"/>
	</xs:simpleType>
	<xs:simpleType name="zipcodeFormatEmpty">
		<xs:union memberTypes="zipcodeFormat empty"/>
	</xs:simpleType>
	<xs:simpleType name="emailAddressFormatEmpty">
		<xs:union memberTypes="emailAddressFormat empty"/>
	</xs:simpleType>
	<xs:simpleType name="NonEmptyString">
		<xs:restriction base="xs:string">
			<xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
		</xs:restriction>
	</xs:simpleType>
