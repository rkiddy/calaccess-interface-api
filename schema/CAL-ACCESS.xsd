                <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
