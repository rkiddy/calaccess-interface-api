              <xs:element name="ReportCoversPeriodFrom" type="dateOrDateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Start of reporting period.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ReportCoversPeriodThrough" type="dateOrDateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    End of reporting period.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="CumulativePeriodBeginning" type="dateOrDateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter Beginning date of cumulative reporting period. Must be on or before beginning date of this report or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="NameOfFiler.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for filer if the filer is an individual. This must 
                    match the name of the filer. Changes may only be made through an amendment to the registration. 
                    Name data on file will be used for generating the statement.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfFiler.FirstName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First Name for the filer if the filer is an individual. This must match the name of the filer. 
                    Changes may only be made through an amendment to the registration. Name data on file will be 
                    used for generating the statement.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfFiler.MiddleName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
