            <xs:sequence>
              <xs:element name="NameOfSlateMailerOrganization" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Provide the name of the Slate Mailer Organization. If this does not match the name of the Slate Mailer Organization 
                    with the ID Number listed in Section0, the registration will be amended and the name will be updated to reflect what is in this file.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="500" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address.Country" minOccurs="0" maxOccurs="1">
