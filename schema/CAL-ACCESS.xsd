<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="CAL-ACCESS" version="1.0.0" xmlns="http://cal-access.sos.ca.gov/" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://cal-access.sos.ca.gov/" >

<xs:include schemaLocation="commonTypes.xsd" />
<xs:include schemaLocation="form460Type.xsd" />

  <!-- Form Definitions - Campaign Finance -->

  <xs:element name="Form400">
    <xs:annotation>
      <xs:documentation xml:lang="en">
        Form 400 - Slate Mailer Organization Statement of Organization
        Use this for amendments to the Statement of Organization for Slate Mailer Organizations. Initial registrations must be done via the online CARS application.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              This section is used to validate that the uploaded file conforms to the expected format, was created by certified software, and is being uploaded to the correct filer.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CARS_Ver" type="xs:string" minOccurs="1" maxOccurs="1" default="">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the version of the XSD file against which this file was validated. This will be used to determine if the XSD file used is still supported.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="CARS_Form" type="xs:string" minOccurs="1" maxOccurs="1" fixed="400">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the CARS form being submitted.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Soft_Name" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the Name of the Vendor of the software used to generate this file. Together with the Software Version, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Soft_Ver" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the version of the software used to generate this file. Together with the Software Name, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="FilerId" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the ID Number assigned to this Slate Mailer Organization. This will be validated against the database to ensure the file is loaded to the correct filer.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Section0" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              Use this section to indicate the level of activity and qualification date for the Slate Mailer Organization.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="IsAmendment" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    If this is an amendment to the registration, mark as true. Note that initial registrations must be submitted in the online CAL-ACCESS system.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="SlateMailerIDNumber" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    ID Number assigned to this Slate Mailer Organization. This must match the FPPCID from the Header section.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="15" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Jurisdiction" type="activityLevelEnumeration" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Activity Levels
                    City
                    State
                    County
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DateQualified" type="dateOrDateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Provide the date the organization received or was promised payments totaling $500 or more in a calendar year for the production of one or more slate mailers.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SectionI" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              Use this section to change information regarding the name and contact information for the Slate Mailer Organization.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="NameOfSlateMailerOrganization" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Provide the name of the Slate Mailer Organization. If this does not match the name of the Slate Mailer Organization 
                    with the ID Number listed in Section0, the registration will be amended and the name will be updated to reflect what is in this file.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="500" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address.Country" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="60" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address.AddressLine1" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    First line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address.City" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address.State" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address.ZipCode" type="zipcodeFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Address.County" type="countyEnumeration" minOccurs="1" maxOccurs="1">
				<xs:annotation>
                  <xs:documentation>
                  	County values verified against enumeration
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PhoneNumber" type="phoneNumberFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Phone number for the person/filer.
                    For phone numbers in the North American Dialing Plan (NADP) region (Country Code of +1), provide a 10-digit phone number. An extension can be added separated from the phone number by an "x". (E.g. (916)555-1234x345).
                    For phone numbers not in the NADP region, provide the country code and phone number as formatted for that country. As with NADP numbers, an extension may be added separated from the phone number by an "x". (E.g. - A number from the United Kingdom would be formatted as +44 1234 5678 x876.)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Fax" type="phoneNumberFormatEmpty" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    FAX number for the person/filer.
                    For FAX numbers in the North American Dialing Plan (NADP) region (Country Code of +1), provide a 10-digit FAX number. An extension can be added separated from the FAX number by an "x". (E.g. (916)555-1234x345).
                    For FAX numbers not in the NADP region, provide the country code and FAX number as formatted for that country. As with NADP numbers, an extension may be added separated from the FAX number by an "x". (E.g. - A number from the United Kingdom would be formatted as +44 1234 5678 x876.)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="EmailAddress" type="emailAddressFormat"  minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Must be formatted as an email address or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="MailingAddressOfFiler.Country" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="60" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="MailingAddressOfFiler.AddressLine1" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    First line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="MailingAddressOfFiler.City" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="MailingAddressOfFiler.State" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="MailingAddressOfFiler.ZipCode" type="zipcodeFormatEmpty" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SectionII" minOccurs="1" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>
              This section is used to identify the Treasurer and other Principal officers of the Slate Mailer Organization. 
              If present in the system TransactionID will be used to match officers. The system will match names entered against names of 
              existing officers to detect changes. If all populated name fields match, the system will look for changes in contact information. 
              If there are mismatches between populated fields but First and Last names match, other fields will be assumed to be changes. 
              Officers lined to the filer that are not mentioned in this amendment will be removed from teh registration and no longer have access to the filer.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and PrincipalOfficer transaction uploaded into CARS. 
                    This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
               <xs:element name="CARS_Id" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and PrincipalOfficer transaction uploaded into CARS. 
                    This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Position" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Insert the position for the named officer. One person must be a Treasurer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfOfficer.FirstName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First Name for the officer.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfOfficer.LastName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Last Name for the officer.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfOfficer.Country" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="60" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfOfficer.Street" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    First line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
                            
              <xs:element name="AddressOfOfficer.City" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfOfficer.State" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfOfficer.ZipCode" type="zipcodeFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PhoneNumber" type="phoneNumberFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Phone number for the person/filer.
                    For phone numbers in the North American Dialing Plan (NADP) region (Country Code of +1), provide a 10-digit phone number. An extension can be added separated from the phone number by an "x". (E.g. (916)555-1234x345).
                    For phone numbers not in the NADP region, provide the country code and phone number as formatted for that country. As with NADP numbers, an extension may be added separated from the phone number by an "x". (E.g. - A number from the United Kingdom would be formatted as +44 1234 5678 x876.)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="EmailAddress" type="emailAddressFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Email address is required for newly added officers so that the system can send them login credentials. 
                    An error will be raised if a newly added officer does not have an email address. Must be formatted as an email address or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SectionIII" minOccurs="1" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>
              Use this section to identify the individuals who authorize the contents of Slate Mailers.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each
                    Individual who authorizes contents of slate mailers transaction uploaded into CAL-ACCESS
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
                <xs:element name="CARS_Id" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and PrincipalOfficer transaction uploaded into CARS. 
                    This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfIndividual.FirstName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First Name for the officer.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfIndividual.LastName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Last Name for the officer.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfIndividual.Country" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="60" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfIndividual.Street" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    First line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfIndividual.City" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfIndividual.State" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfIndividual.ZipCode" type="zipcodeFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PhoneNumber" type="phoneNumberFormat"  minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Phone number for the person/filer.
                    For phone numbers in the North American Dialing Plan (NADP) region (Country Code of +1), provide a 10-digit phone number. An extension can be added separated from the phone number by an "x". (E.g. (916)555-1234x345).
                    For phone numbers not in the NADP region, provide the country code and phone number as formatted for that country. As with NADP numbers, an extension may be added separated from the phone number by an "x". (E.g. - A number from the United Kingdom would be formatted as +44 1234 5678 x876.)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="EmailAddress" type="emailAddressFormat" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Email address is required for newly added officers so that the system can send them login credentials. 
                    An error will be raised if a newly added officer does not have an email address. Must be formatted as an email address or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>              
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SectionIV" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              Use this section to indicate whether or not this Slate Mailer Organization is considered a "Committee" pursuant to Government Code Section 82013. If so, indicate the name and ID Number of the Committee.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="IsCommittee" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Mark as true if the organization is a Committee.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Name" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Provide the name of the committee. This is required if IsCommittee = TRUE. 
                    This must match the name of the committee registered in the CAL-ACCESS system with the ID Number listed in IDNumber field in this section or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="500" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="IDNumber" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Provide the ID Number of the committee. This is required if IsCommittee = TRUE. 
                    This must match the ID Number of the committee registered in the CAL-ACCESS system with the Name listed in Name field in this section or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="15" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SectionV" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              This section contains the signature attesting that the information contained in this filing is complete and accurate. 
              The person signing this must be listed as an active officer of the Slate Mailer Organization
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each
                    Individual who authorizes contents of slate mailers transaction uploaded into CAL-ACCESS
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="CARS_Id" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and Principal Officer  
                    uploaded into CARS. This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="ExecutedOn" type="dateOrDateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Insert the date on which the signature was obtained. No future dates are allowed.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ExecutedAt.City" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    City and State where the signature was obtained
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>                
              </xs:element>
              <xs:element name="ExecutedAt.State" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    City and State where the signature was obtained
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>                
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.FirstName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First name of Responsible Officer. Must match the name of an active Responsible Officer linked to the filer 
                    or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.LastName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Last name (surname) of Responsible Officer. 
                    Must match the name of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <!-- removed NameOfResponsibleOfficer.Title as role is referred from sec2 -->
              <!-- <xs:element name="PenaltyofPerjury" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
				   PenaltyofPerjury
                  </xs:documentation>
                </xs:annotation>
              </xs:element>  -->             
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Form401">
    <xs:annotation>
      <xs:documentation xml:lang="en">
        Form 401 - Slate Mailer Organization Campaign Statement
        A Slate Mailer Organization will use this to disclose payments made and received in connection with producing slate mailers.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              This section is used to validate that the uploaded file conforms to the expected format, was created by certified software, and is being uploaded to the correct filer.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CAL-ACCESS_Ver" type="xs:string" minOccurs="1" maxOccurs="1" default="1.07">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the version of the XSD file against which this file was validated. This will be used to determine if the XSD file used is still supported.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="CAL-ACCESS_Form" type="xs:string" minOccurs="1" maxOccurs="1" fixed="401">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the CAL-ACCESS form being submitted.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Soft_Name" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the Name of the Vendor of the software used to generate this file. Together with the Software Version, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Soft_Ver" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the version of the software used to generate this file. Together with the Software Name, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="FPPCID" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the ID Number assigned to this Slate Mailer Organization. This will be validated against the database to ensure the file is loaded to the correct filer.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Section0" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              This section is used to define the reporting period covered by the report and to indicate whether or not the report is an amendment of an earlier version.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="StatementCoversPeriodFrom" type="dateOrDateTime" minOccurs="1" maxOccurs="1" >
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Start of reporting period.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="StatementCoversPeriodThrough" type="dateOrDateTime" minOccurs="1" maxOccurs="1" >
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    End of reporting period.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="IsAmendment" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Set to True if this is an amended version of a previously filed report. If true, the report cover period must match the previously filed report and an explanation must be provided.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="AmendmentExplanation" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Provide an explanation of why the report is being amended.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SectionI" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              This section is used to identify the filer. The information in this section must match that of the filer for whom the report is being uploaded or errors will be raised.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="IDNumber" minOccurs="1" maxOccurs="1" >
                <xs:annotation>
                  <xs:documentation>
                    ID Number assigned to this Slate Mailer Organization. This must match the FPPCID from the Header section.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="15" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfSlateMailerOrganization" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Provide the name of the Slate Mailer Organization. Changes may only be made through an amendment to the registration. 
                    Name data on file will be used for generating the statement. An error will be raised  if the name here does not match that of the filer.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="500" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!--<xs:element name="SectionII" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CommitteeReportAttached" type="xs:boolean" minOccurs="1" maxOccurs="1" />
              <xs:element name="IDNumber" type="xs:string" minOccurs="0" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>-->
        <!--<xs:element name="SectionIII" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Line1ColumnA" type="xs:decimal" minOccurs="1" maxOccurs="1" />
              <xs:element name="Line1ColumnB" type="xs:decimal" minOccurs="1" maxOccurs="1" />
              <xs:element name="Line2ColumnA" type="xs:decimal" minOccurs="1" maxOccurs="1" />
              <xs:element name="Line2ColumnB" type="xs:decimal" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>-->
        <xs:element name="SectionIV" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              This section contains the signature attesting that the information contained in this filing is complete and accurate. The person signing this must be listed as officers for the filer or an error will be raised.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ExecutedOn" type="dateOrDateTime" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Insert the date on which the signature was obtained. No future dates are allowed.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ExecutedAt" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter thelocation of the Responsible Officer at the time of attestation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for Responsible Officer. 
                    Must match the name of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.FirstName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First name of Responsible Officer. 
                    Must match the name of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.MiddleName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Middle name(s) or initial(s) of Responsible Officer. 
                    Must match the name of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.LastName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Last name (surname) of Responsible Officer. 
                    Must match the name of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.NameSuffix" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Name suffix (e.g. JR, SR, III, PhD, etc.) of Responsible Officer. 
                    Must match the name of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfResponsibleOfficer.Title"  minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Include the title of the signatory. 
                    Must match the title of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="ScheduleA" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Use this section to report paymennts received to produce slate mailers.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Payment Received transaction uploaded into CAL-ACCESS.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="DateReceived" type="dateOrDateTime" minOccurs="1" maxOccurs="1" >
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Date the payment was received. Must be on or after the beginning of the reporting period and on or before the ending date
                    of the reporting period or a warning will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PayerType" type="PayerTypeEnumeration" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Valid values:
                    CMT - Committee
                    COH - Candidate/Officeholder
                    IND - Individual
                    OTH - Other (e.g. business entity)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PayerName.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for payer if the payer is an individual or candidate/officeholder.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PayerName.FirstName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First Name for the payer if the payer  is an individual or candidate/officeholder.
                    Required if PayerType = IND or COH.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PayerName.MiddleName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Middle Name or Initial for the payer if the payer is an individual or candidate/officeholder.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PayerName.LastName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Last Name for the payer if the payer is an individual or candidate/officeholder.
                    Required if PayerType = IND or COH.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PayerName.NameSuffix" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Name Suffix  (e.g. JR, SR, III, PhD, etc.) for the payer if the payer is an individual or candidate/officeholder.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PayerName.CompanyName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Name of the  payer if the payer is neither an individual, candidate/offceholder or a committee.
                    Required if ContributorType = OTH.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PayerName.CommitteeName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Name of the committee if the payer is a committee.
                    Required if PayerType = CMT.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PayerName.CommitteeID" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Filer ID for the committee making the payment. If the payment is from a recipient committee, the Filer ID of the Committee must be provided.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="15" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="IndividualOccupation" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the occupataion of the payer if the payer is an individual. 
                    A warning will be raised if this field is empty and the EmployerInformationRequested field is set to False.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="250" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="IndividualEmployer" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the employer of the payer if the payer is an individual. 
                    A warning will be raised if this field is empty and the EmployerInformationRequested field is set to False.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="EmployerInformationRequested" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Set this field to True if the employment information of an individual payer is not present but has been requested. 
                    A warning will be raised if the value for this field is False and no payer employtment information is present.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Candidate.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for candidate if the payment is to support or oppose a candidate other than the payer.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
