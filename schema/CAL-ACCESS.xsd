        </xs:element>
        <xs:element name="PartII-SecA-ActivityExpense" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
