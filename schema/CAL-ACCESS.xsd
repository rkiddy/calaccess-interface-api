              <xs:element name="SponsorAddress.State" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="SponsorAddress.ZipCode" type="zipcodeFormat" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>              
              <xs:element name="Sponsor.PhoneNumber" type="phoneNumberFormat" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Phone number for the person/filer.
                    For phone numbers in the North American Dialing Plan (NADP) region (Country Code of +1), provide a 10-digit phone number. An extension can be added separated from the phone number by an "x". (E.g. (916)555-1234x345).
                    For phone numbers not in the NADP region, provide the country code and phone number as formatted for that country. As with NADP numbers, an extension may be added separated from the phone number by an "x". (E.g. - A number from the United Kingdom would be formatted as +44 1234 5678 x876.)
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Section4-SmallContributorCommittee" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Use this section to indicate that a committee is a Small Contributor Committee by entering the date of qualification.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="DateQualified" nillable="true"  minOccurs="0" maxOccurs="1" >
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the date the committee qualified as a small contributor committee.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:list itemType="xs:date"/>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Form425">
