                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="LobbyistName.LastName" minOccurs="0" maxOccurs="1">
