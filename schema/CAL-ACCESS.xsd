              <xs:element name="LineE" type="xs:decimal" minOccurs="1" maxOccurs="1" />-->
              <xs:element name="CampaignContributionsMade" type="campaignContributionsMadeEnumeration" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Valid values: "None This Period" or "Part IV Completed and Attached". If "Part IV Completed and Attached" is selected, an error will be raised 
                    if there are no PartIV-SectionA or PartIV-SectionB records.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PartI-Activities" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              This section is used to report the lobbying activities performed.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Activity transaction uploaded into CAL-ACCESS. 
                    This ID will be used to track any amendments to the transactions.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="ActivityType" type="activityTypeEnumeration" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Type of activity. Valid choices are "Legislative" or "Administrative".
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="BillNumber" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Type of activity. Valid choices are "Legislative" or "Administrative".
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="StateAgency" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Name of State Agency lobbyied. Required if ActivityType = Administrative.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RegulationNumber" type="xs:string" minOccurs="0" maxOccurs="1" >
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Regulation number lobbyied. Required if ActivityType = Administrative.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DescriptionOfActivity" type="xs:string" minOccurs="0" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PartII-LobbyistReports" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Use this section to report the names of partners, owners, and employees whose "Lobbyist Reports" (Form 615) are attached to this report.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each transaction uploaded into CAL-ACCESS. 
                    This ID will be used to track any amendments to the transactions.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PartnerOwnerEmployee.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PartnerOwnerEmployee.FirstName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First Name for the person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PartnerOwnerEmployee.MiddleName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Middle Name or Initial for person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PartnerOwnerEmployee.LastName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Last Name for the person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PartnerOwnerEmployee.NameSuffix" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Name Suffix (e.g. JR, SR, III, PhD, etc.) for the the person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PartnerOwnerEmployee.Title" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Business title of the person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PartIII-SecA-InhousePayments" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Use this section to report payment to in-house lobbyists for the reporting period.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="AmountThisPeriod" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Report amount paid to in-house lobbyists. May be 0.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <!--<xs:element name="CumulativeTotalToDate" type="xs:decimal" minOccurs="1" maxOccurs="1" />-->
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PartIII-SecB-LobbyingFirmPayments" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Use this section to report payments to lobbying firms (including individual contract lobbyists).
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Lobbying Firm Payments transaction uploaded into CAL-ACCESS. 
                    This ID will be used to track any amendments to the transactions.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfLobbyingFirm" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the name of the lobbying firm or independent contractor. This must be the name by which they are registerred in the system or an error will be raised. 
                    They must be linked to the Employer or Coalition or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="500" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="FeesAndRetainers" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Amount paid for Fees and Retainers. Enter 0 if no fees and retainers were paid to this firm or contractor.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="ReimbursementsOfExpenses" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Amount paid for Reimbursements fo Expenses. Enter 0 if no reimbursements were paid to this firm or contractor.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="AdvancesOrOtherPaymentsAmount" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Amount paid for Advances or Other Payments. Enter 0 if no advances or other payments were paid to this firm or contractor. If not 0, then an explanation is required.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="AdvancesOrOtherPaymentsExplanation" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Explanation for any amount reported in AdvancesOrOtherPaymentsAmount field. An explanation is required if the amount is not 0 or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="4000" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <!--<xs:element name="CumulativeTotalToDate" type="xs:decimal" minOccurs="1" maxOccurs="1" />-->
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!--<xs:element name="PartIII-SectionB-Summary" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TotalOfSectionB" type="xs:decimal" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>-->
        <xs:element name="PartIII-SecC-ActivityExpense" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              This section is used to report payments made in connection with lobbying activities. Reportable persons linked to those activities are reported in 
              the PartIII-SecC-ReportablePerson section and linked via TransactionID and ParentTransctionID fields.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each 
                    Activity Expense transaction uploaded into CAL-ACCESS. This ID will be used to link Reportable Persons to the expense and to track any amendments to the transactions.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Date" type="dateOrDateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Date the activity expnse was paid.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="NameOfPayee" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Full name of the payee for the activity expense.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="500" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.Country" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="60" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine1" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First line of address. May not be a PO Box.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine2" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Second Line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Third line of address. Not used for US addresses.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine4" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Fourth line of address. Not used for US addresses.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.City" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.State" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element. 
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.ZipCode" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc). 
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="28" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="DescriptionOfConsideration" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Describe the activity expense. If the firm paid a porton of the expense and others paid the remainder, explain here.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="4000" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="TotalAmountOfActivity" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Total amount paid for the activity. This must be equal to or greater than the total of the AmountBenefitingReportablePerson for the 
                    reportable person records tied to this activity expense or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PartIII-SecC-ReportablePerson" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              This section is used to list the reportable persons benefiting from the activity expenses reported during this period.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ParentTransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    ParentTransactionID is the unique identifier and reference number for 
                    each Parent transaction uploaded into CAL-ACCESS. Specifically, this should reference a record in PartIII-SecA-ActivityExpense.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for 
                    each Reportable Person transaction uploaded into CAL-ACCESS. This ID will be used to track any amendments to the transactions.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfReportablePerson.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for Reportable Person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfReportablePerson.FirstName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First name of Reportable Person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfReportablePerson.MiddleName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Middle name(s) or initial(s) of Reportable Person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfReportablePerson.LastName" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Last name (surname) of Reportable Person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfReportablePerson.NameSuffix" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Name suffix (e.g. JR, SR, III, PhD, etc.) of Reportable Person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="OfficialPositionOfReportablePerson" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    State the official position of this person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AmountBenefitingReportablePerson" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    State how much of the activity expense benefited this person. Total of this field for all persons benefitting from the same activity expense must be 
                    less than nor equal to the total amount of the activity expense or an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DescriptionOfConsideration" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Describe the consideration received by the reportable person.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="4000" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!--<xs:element name="PartIII-SectionC-Summary" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TotalSectionC-ActivityExpenses" type="xs:decimal" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>-->
        <xs:element name="PartIII-SectionD" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Use this section to report other payments to influence legislative or administrative action.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Line1" type="xs:decimal" minOccurs="1" maxOccurs="1" >
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Report payments made to Lobbying Coalitions here. This may be 0. If greater than 0, a Form630 must be attached. If less than 0 an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Line2" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Report other payments made here. This may be 0. If greater than 0, a Form640 must be attached. If less than 0 an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <!--<xs:element name="TotalSectionD" type="xs:decimal" minOccurs="1" maxOccurs="1" />-->
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PartIII-SectionE" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              Use this section to report payments made in connection with administrative testimony in ratemaking proceedings before the California Public Utilities Commission.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PaymentsInRatemakingProceedings" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Report payments here. This may be 0. If less than 0 an error will be raised.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PartIV-ContributionsMade-SecA" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              This section is used to report the name and ID of a Major Donor or Recipient Committee through which campaign contributions made by the filer have been reported to the SOS. 
              It is required if CampaignContributionsMade in PartI is set to "Part IV Completed and Attached" and contributions were reported through a Major Donor or Recipient Committee.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="MajorDonor.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
