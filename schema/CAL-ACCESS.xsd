                </xs:simpleType>
              </xs:element>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Activity transaction uploaded into CAL-ACCESS. This 
                    ID will be used to track any amendments to the transactions.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
