                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="FilerOrResponsibleOfficer.LastName" minOccurs="0" maxOccurs="1">
