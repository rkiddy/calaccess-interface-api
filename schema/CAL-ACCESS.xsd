              <xs:element name="Line2" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Total payments to Lobbying Coalitions. Report as a lump sum. (Form 630 must be attached). Enter 0 if no payments to Lobbying Coalitions were made.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Line3" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Total payments of less than $250 or $2,500, as applicable, during the calendar quarter for lobbying activity 
                    (excluding overhead). Report as a lump sum. Enter 0 if no applicable payments were made.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <!--<xs:element name="Line4" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Line5" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                  </xs:documentation>
                </xs:annotation>
              </xs:element>-->
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Payments" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each
                    transaction uploaded into CAL-ACCESS
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="PaymentCode" type="paymentCodeEnumeration" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Payment Codes:
                    [S] for salary of employees other than lobbyists who are engaged for 10% or more of 
                    their time in one month in activities related to lobbying;
                    [E] for expenses incurred by a lobbyist and paid directly by the filer, or expenses 
                    incurred by the filer for goods or services used by a lobbyist;
                    [L] for legislative related-services performed by a lobbying firm under Regulation 
                    18614(b)(2);
                    [C] for consultants and government relations;
                    [P] for public affairs, including coalition building and grassroots campaigns;
                    [A] for advertising;
                    [R] for research including feasibility studies, analysis, polling and public opinion 
                    research;
                    [V] for lobbying events including event planning, rentals, equipment, and 
                    transportation for members or the public;
                    [O] for all other payments not covered by one of the specific categories.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="NameOfPayee.NameTitle" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for Payee.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfPayee.FirstName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    First Name for the Payee. Required if the Payee is an individual. An error will be raised if FirstName, LastName, and Name are all empty.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfPayee.MiddleName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Middle Name or Initial for the Payee.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfPayee.LastName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Last Name for the Payee. Required if the Payee is an individual. An error will be raised if FirstName, LastName, and Name are all empty.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfPayee.NameSuffix" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Name Suffix (e.g. JR, SR, III, PhD, etc.) for the Payee.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfPayee.Name" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Enter the Name for the Payee if the Payee is not an individual. Either an individuals name 
                    must be entered, or a name of a business or organization must be entered. An error will be 
                    raised if FirstName, LastName, and Name are all empty.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="500" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.Country" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="60" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine1" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    First line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine2" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Second line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Third line of address. Not used for US addresses.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine4" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Fourth line of address. Not used for US addresses.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.City" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.State" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.ZipCode" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                    <xs:maxLength value="28" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AmountThisQuarter" type="xs:decimal" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Enter the amount of payments to this payee during this reporting period. Must be a positive number.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <!--<xs:element name="CumulativeAmountToDateCalendarYear" type="xs:decimal" minOccurs="1" maxOccurs="1" />-->
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!--<xs:element name="Subtotal" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="SubtotalAmountThisQuarter" type="xs:decimal" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>-->
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="Form645">
    <xs:annotation>
      <xs:documentation xml:lang="en">
        Form 645 - Report of Persons Spending $5,000 or More to Influence Legislative or Administrative Action
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              This section is used to validate that the uploaded file conforms to the expected format, was created by certified software, and is being uploaded to the correct filer.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CAL-ACCESS_Ver" type="xs:string" minOccurs="1" maxOccurs="1" default="1.07">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the version of the XSD file against which this file was validated. This will be used to determine if the XSD file used is still supported.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="CAL-ACCESS_Form" type="xs:string" minOccurs="1" maxOccurs="1" fixed="645">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the CAL-ACCESS form being submitted.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Soft_Name" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the Name of the Vendor of the software used to generate this file. Together with the Software Version, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Soft_Ver" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the version of the software used to generate this file. Together with the Software Name, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="FPPCID" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    Provide the ID Number assigned to this Lobbying Filing Entity. This will be validated against the database to ensure the file is loaded to the correct filer.
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Part0" minOccurs="1" maxOccurs="1">
