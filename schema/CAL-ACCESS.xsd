                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="NameOfFiler.LastName" minOccurs="0" maxOccurs="1">
                <xs:annotation>
