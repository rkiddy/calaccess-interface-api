<xs:complexType name="verificationType">
	<xs:annotation>
		<xs:documentation xml:lang="en">
			This section contains the signatures attesting that the information contained in this filing is complete and accurate.
			The person(s) signing this must be listed as an officer for the filer.
		</xs:documentation>
	</xs:annotation>
	<xs:sequence>
		<xs:element name="TransactionID" minOccurs="1" maxOccurs="1">
			<xs:annotation>
				<xs:documentation xml:lang="en">
					TransactionID is the unique identifier and reference number for each
					transaction uploaded into CAL-ACCESS
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="NonEmptyString">
					<xs:maxLength value="100"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:element name="CARS_Id" minOccurs="1" maxOccurs="1">
			<xs:annotation>
				<xs:documentation xml:lang="en">
					TransactionID is the unique identifier and reference number for each Treasurer and Principal Officer
					uploaded into CARS. This will be used as the identifier for the officers in subsequent amendments.
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
					<xs:maxLength value="100"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:element name="ExecutedOn" type="dateOrDateTime" minOccurs="1" maxOccurs="1">
			<xs:annotation>
				<xs:documentation xml:lang="en">
					Insert the date on which the signature was obtained. No future dates are allowed.
				</xs:documentation>
			</xs:annotation>
		</xs:element>
		<xs:element name="SignerType" minOccurs="0" maxOccurs="1">
			<xs:annotation>
				<xs:documentation xml:lang="en">
					Signer type (Candidate, Treasurer, Principal Officer, etc.)
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:element name="Name.FirstName" minOccurs="0" maxOccurs="1">
			<xs:annotation>
				<xs:documentation xml:lang="en">
					First name of person attesting.
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:element name="Name.LastName" minOccurs="0" maxOccurs="1">
			<xs:annotation>
				<xs:documentation xml:lang="en">
					Last name (surname) of person attesting.
				</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="100"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
	</xs:sequence>
</xs:complexType>
