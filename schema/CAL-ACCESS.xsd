                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.Country" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="60" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine1" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    First line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine2" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Second line of address.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine3" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Third line of address. Not used for US addresses.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.AddressLine4" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Fourth line of address. Not used for US addresses.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="200" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.City" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.State" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AddressOfPayee.ZipCode" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
