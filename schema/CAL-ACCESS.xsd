  <xs:element name="Form511">
    <xs:annotation>
      <xs:documentation xml:lang="en">
        Form 511 - Paid Spokesperson Report
        Use this to report expenditures made to spokespersons in certain circumstances.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Header" minOccurs="1" maxOccurs="1">
