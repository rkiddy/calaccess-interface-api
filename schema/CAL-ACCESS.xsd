			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string"> 
					<xs:maxLength value="300"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
	</xs:sequence>
</xs:complexType>
