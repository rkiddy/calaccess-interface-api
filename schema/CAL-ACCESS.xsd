<xs:complexType name="committeeType">
	<xs:sequence>
		<xs:element name="Name" minOccurs="0" maxOccurs="1">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="300"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:choice>
			<xs:annotation>
				<xs:documentation xml:lang="en">
					if the committee does not yet have an id value, provide the treasurer's contact information
				</xs:documentation>
			</xs:annotation>
			<xs:element name="CommitteeId" minOccurs="0" maxOccurs="1">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Treasurer" type="treasurerType" minOccurs="0" maxOccurs="1"/>
		</xs:choice>
	</xs:sequence>
</xs:complexType>
<xs:complexType name="organizationType">
	<xs:sequence>
		<xs:element name="Name" minOccurs="1" maxOccurs="1">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="300"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
	</xs:sequence>
</xs:complexType>
<xs:complexType name="treasurerType">
	<xs:complexContent>
		<xs:extension base="individualType">
			<xs:sequence>
				<xs:element name="address" type="addressType" minOccurs="0" maxOccurs="1"/>
				<xs:element name="phone" type="phoneNumberFormat" minOccurs="0" maxOccurs="1"/>
				<xs:element name="fax" type="phoneNumberFormat" minOccurs="0" maxOccurs="1"/>
				<xs:element name="email" type="emailAddressFormat" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
		</xs:extension>
	</xs:complexContent>
</xs:complexType>
