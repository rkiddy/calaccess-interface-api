    <xs:simpleType name="zipcodeFormat">
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{5}(-[0-9]{4})?"/>
            <xs:maxLength value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="empty">
        <xs:restriction base="xs:string">
            <xs:maxLength value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="stateFormat">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CA"/>
        </xs:restriction>
    </xs:simpleType>
    <!--  (123) 456-7890 | 123-456-7890 -->
    <xs:simpleType name="phoneNumberFormat">
        <xs:restriction base="xs:string">
            <xs:pattern value="((\(\d{3}\) ?)|(\d{3}-))?\d{3}-\d{4}"/>
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="emailAddressFormat">
        <xs:restriction base="xs:string">
            <xs:pattern value="[^@]+@[^\.]+\..+"/>
            <xs:maxLength value="250"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="phoneNumberFormatEmpty">
        <xs:union memberTypes="phoneNumberFormat empty"/>
    </xs:simpleType>
    <xs:simpleType name="zipcodeFormatEmpty">
        <xs:union memberTypes="zipcodeFormat empty"/>
    </xs:simpleType>
    <xs:simpleType name="emailAddressFormatEmpty">
        <xs:union memberTypes="emailAddressFormat empty"/>
    </xs:simpleType>
    <xs:simpleType name="NonEmptyString">
        <xs:restriction base="xs:string">
            <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
        </xs:restriction>
    </xs:simpleType>
