                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <!-- <xs:element name="SponsorAddress.Country" minOccurs="1" maxOccurs="1">
                <xs:annotation>
