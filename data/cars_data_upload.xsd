                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="AddressOfPayee.City">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="AddressOfPayee.State">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="10"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="AddressOfPayee.ZipCode">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="28"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="AmountThisQuarter" type="xs:decimal">
                                <xs:annotation>
                                    <xs:documentation>
                    Enter the amount of payments to this payee during this reporting period. Must be a positive number.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <!--<xs:element name="CumulativeAmountToDateCalendarYear" type="xs:decimal" minOccurs="1" maxOccurs="1" />-->
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
