                                <xs:annotation>
                                    <xs:documentation>
                    Enter the total amount spent on the activity. Must be equal to or greater than the amount benefitting the reportable
                    persions linked to this activity expense or an error will be raised for the record.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="PartII-SecA-ReportablePerson">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              Use this section to list persons benefitting from an Activity Expense.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="TransactionID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is used to link amended versions of the Reportable Person to the originally filed version.
                    This must be unique within the file and may not change between versions.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
