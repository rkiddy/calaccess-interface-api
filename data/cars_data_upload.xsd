            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="TransactionID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Payment Received transaction uploaded into CAL-ACCESS.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="DateReceived" type="dateOrDateTime">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Date the payment was received. Must be on or after the beginning of the reporting period and on or before the ending date
                    of the reporting period or a warning will be raised.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="PayerType" type="PayerTypeEnumeration">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Valid values:
                    CMT - Committee
                    COH - Candidate/Officeholder
                    IND - Individual
                    OTH - Other (e.g. business entity)
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.NameTitle">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for payer if the payer is an individual or candidate/officeholder.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.FirstName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    First Name for the payer if the payer  is an individual or candidate/officeholder.
                    Required if PayerType = IND or COH.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
