                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <!--   <xs:element name="PenaltyofPerjury" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                            PenaltyofPerjury
                  </xs:documentation>
                </xs:annotation>
              </xs:element> -->
                            <xs:element maxOccurs="1" minOccurs="0" name="ExecutedOn" type="dateOrDateTime">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Insert the date on which the signature was obtained. No future dates are allowed.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="ExecutedAt.City">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    City where the signature was obtained
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="ExecutedAt.State">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                     State where the signature was obtained
                  </xs:documentation>
