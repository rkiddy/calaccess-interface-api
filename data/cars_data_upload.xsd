                                    <xs:documentation xml:lang="en">
                    Enter the effective date of the amendment.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="DeletingLobbyistEmployer" type="xs:boolean">
