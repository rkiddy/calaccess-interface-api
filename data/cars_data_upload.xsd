                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Address.Country">
                                <xs:annotation>
                                    <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                                </xs:annotation>
