                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="ExecutedOn" type="dateOrDateTime">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Insert the date on which the signature was obtained. No future dates are allowed.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="ExecutedAt.City">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    City and State where the signature was obtained
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="ExecutedAt.State">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    City and State where the signature was obtained
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="NameOfResponsibleOfficer.FirstName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    First name of Responsible Officer. Must match the name of an active Responsible Officer linked to the filer
                    or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="NameOfResponsibleOfficer.LastName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Last name (surname) of Responsible Officer.
                    Must match the name of an active Responsible Officer linked to the filer or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
