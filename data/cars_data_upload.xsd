                                    <xs:documentation xml:lang="en">
                    First name of Responsible Officer. Must match the name of an active Responsible Officer linked to the
                    filer or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="NameOfResponsibleOfficer.MiddleName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Middle name(s) or initial(s) of Responsible Officer. Must match the name of an active Responsible Officer
                    linked to the filer or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="NameOfResponsibleOfficer.LastName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Last name (surname) of Responsible Officer. Must match the name of an active Responsible Officer linked
                    to the filer or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
