                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.FirstName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    First Name for the Candidate supported or opposed.
                    Either a First and Last Name of a Candidate or the Name of a Ballot Measure must be supplied or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.MiddleName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Middle Name or Initial for the Candidate supported or opposed.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.LastName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Last Name for the Candidate supported or opposed.
                    Either a First and Last Name of a Candidate or the Name of a Ballot Measure must be supplied or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.NameSuffix">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Name Suffix  (e.g. JR, SR, III, PhD, etc.) for the Candidate supported or opposed.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Office">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Office sought or held by the candidate.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Measure">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Number or ID assigned to the Ballot Question
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="MeasureTitle">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Title of the Ballot Question.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Jurisdiction">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Jurisdiction of the Ballot Question or Candidate.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="District">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    District in which the Candidate is running.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="AmountOfContribution" type="xs:decimal">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Amount contributed. Must be a non-zero, positive number.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="LoanCheckBox" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>
                    Use this field to indicate that the Contribution Made took the form of a Loan Made so that the system can track the loan correctly.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="DateOfElection" type="dateOrDateTime">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the date of the election for the candidate or measure.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="2" minOccurs="0" name="AmendmentExplanation">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              Use this section to explain the nature of the amendment if this report is amending an earlier version.
              Include separate explanatinofor each of section 1 and 2 and indicate to which section the explanation pertains.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="Section">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter &quot;Section 1&quot; or &quot;Section 2&quot;
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="10"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Explanation">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the explanation of the changes to this section for this amendment.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="4000"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Form498">
        <xs:annotation>
            <xs:documentation xml:lang="en">
        Form 498 - Late Payment Report
        Slate Mailer Organizations use this to report payments of $2,500 or more from a single source to pay for a slate mailer supporting or opposing and candidate or ballot measure during the 90 days prior to and ending on the election date for the candidate or measure.
      </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="Header">
                    <xs:annotation>
                        <xs:documentation>
              This section is used to validate that the uploaded file conforms to the expected format, was created by certified software, and is being uploaded to the correct filer.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element default="1.07" maxOccurs="1" minOccurs="1" name="CAL-ACCESS_Ver" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the version of the XSD file against which this file was validated. This will be used to determine if the XSD file used is still supported.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element fixed="498" maxOccurs="1" minOccurs="1" name="CAL-ACCESS_Form" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the CAL-ACCESS form being submitted.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Soft_Name" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the Name of the Vendor of the software used to generate this file. Together with the Software Version, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Soft_Ver" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the version of the software used to generate this file. Together with the Software Name, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="FPPCID" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the ID Number assigned to this Slate Mailer Organization. This will be validated against the database to ensure the file is loaded to the correct filer.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="1" name="Section0">
                    <xs:annotation>
                        <xs:documentation>
              Use this section for base information about the filing.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="IsAmendment" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>
                    If this is an amendment to the registration, mark as true. Note that original registrations must be submitted in the online CAL-ACCESS system.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="AmendmentNumber">
                                <xs:annotation>
                                    <xs:documentation>
                    Number for which amendment to the original report. Must be provided if IsAmendment is True.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="25"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="ReportNumber">
                                <xs:annotation>
                                    <xs:documentation>
                    Transaction number for original report. Must be provided if IsAmendment is True.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="25"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="NameOfSlateMailerOrganization.Name">
                                <xs:annotation>
                                    <xs:documentation>
                    Name of the committee. Changes may only be made through an amendment to the registration. Name data on file
                    will be used for generating the statement.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="500"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SlateMailerID">
                                <xs:annotation>
                                    <xs:documentation>
                    Filer ID number of the committee (or candidate, if filed by a  candidate without a committee).
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="15"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="1" name="PaymentReceivedFrom">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              Use this section to list the entity from whom the late payment was received.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="TransactionID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is used to relate Late Payment items to Candidate or Ballot Measure items. It is also used
                    to relate this Payment Received transaction to the same transaction when that transaction is filed on a
                    form 401.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="ContributorType" type="sourceCodeEnumeration">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Contributor Codes
                    IND – Individual
                    CMT – Recipient Committee
                    OTH – Other (e.g., business entity)
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.NameTitle">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for payer if the payer is an individual.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.FirstName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    First Name for the payer if the payer  is an individual .
                    Required if ContributorType = IND.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.MiddleName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Middle Name or Initial for the payer if the payer is an individual.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.LastName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Last Name for the payer if the payer  is an individual .
                    Required if ContributorType = IND.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.NameSuffix">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Name Suffix  (e.g. JR, SR, III, PhD, etc.) for the payer if the payer is an individual.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="PayerName.Name">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Name of the  payer if the payer is neither an individual or a committee.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Payer.CommitteeName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Name of the committee if the payer is a committee.
                    Required if ContributorType = COM, PTY, or SCC.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="IDNumber">
                                <xs:annotation>
                                    <xs:documentation>
                    Filer ID for the committee making the payment.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="15"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Address.Country">
                                <xs:annotation>
                                    <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="60"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Address.AddressLine1">
                                <xs:annotation>
                                    <xs:documentation>
                    First line of address.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Address.AddressLine2">
                                <xs:annotation>
                                    <xs:documentation>
                    Second line of address.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Address.AddressLine3">
                                <xs:annotation>
                                    <xs:documentation>
                    Third line of address. Not used for US addresses.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Address.AddressLine4">
                                <xs:annotation>
                                    <xs:documentation>
                    Fourth line of address. Not used for US addresses.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Address.City">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Address.State">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="10"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Address.ZipCode">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="28"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="OccupationOrNameOfBusiness" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>
                    The name of the payor's employer and occupation, or name of the payor's business if self-employed. Required if payor is an individual.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="DateReceived" type="dateOrDateTime">
                                <xs:annotation>
                                    <xs:documentation>
                    Date the payment was received.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Amount" type="xs:decimal">
                                <xs:annotation>
                                    <xs:documentation>
                    Amount of the payment. Must be a positive number.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="CandidateOrBallotMeasure">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              Use this section to record how the late payment is allocated between various candidates and/or ballot measures.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="ParentTransactionID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    ParentTransactionID is used to relate Late Payment items to Candidate or Ballot Measure items. Must match
                    the TransactionID from the PaymentReceivedFrom section.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="TransactionID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Candidate Or Ballot Measure
                    transaction uploaded into CAL-ACCESS
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.NameTitle">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Courtesy title (e.g. MS, MR, DR, etc.) used for Candidate supported or opposed.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.FirstName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    First Name for the Candidate supported or opposed.
                    Either a First and Last Name of a Candidate or the Name of a Ballot Measure must be supplied or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.MiddleName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Middle Name or Initial for the Candidate supported or opposed.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.LastName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Last Name for the Candidate supported or opposed.
                    Either a First and Last Name of a Candidate or the Name of a Ballot Measure must be supplied or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Candidate.NameSuffix">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the Name Suffix  (e.g. JR, SR, III, PhD, etc.) for the Candidate supported or opposed.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="NameOfBallotMeasure">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Name of the ballot measure supported or opposed. Either a First and Last Name of a Candidate or the
                    Name of a Ballot Measure must be supplied or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SupportOppose" type="supportOpposeEnumeration">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Valid values are &quot;Support&quot; or &quot;Oppose&quot;.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="OfficeSought">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Office soughtor held for the candidate supported or opposed. Required if Supporting/opposing a candidate.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Jurisdiction">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Jurisdiction for the measure supported or opposed.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="District">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    District, if any, for the office sought or held
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="AmountAttributed" type="xs:decimal">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Amount attributed to this candidate or ballot measure. Total of all AmountAttributed records must be
                    less than or equal to the amount of the PaymentReceivedFrom record or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- Form Definitions - Other -->
    <xs:element name="Form511">
        <xs:annotation>
            <xs:documentation xml:lang="en">
        Form 511 - Paid Spokesperson Report
        Use this to report expenditures made to spokespersons in certain circumstances.
      </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="Header">
                    <xs:annotation>
                        <xs:documentation>
              This section is used to validate that the uploaded file conforms to the expected format, was created by certified software, and is being uploaded to the correct filer.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element default="1.07" maxOccurs="1" minOccurs="1" name="CARS_Ver" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the version of the XSD file against which this file was validated. This will be used to determine if the XSD file used is still supported.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element fixed="511" maxOccurs="1" minOccurs="1" name="CARS_Form" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the CAL-ACCESS form being submitted.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Soft_Name" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the Name of the Vendor of the software used to generate this file. Together with the Software Version, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Soft_Ver" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the version of the software used to generate this file. Together with the Software Name, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="FilerId" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the ID Number assigned to this Slate Mailer Organization. This will be validated against the database to ensure the file is loaded to the correct filer.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="1" name="Section0">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              Use this section to identify the filer and the report.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="CommitteeName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Full name of the filer. This must match the name of the filer. Changes may only be made through an
                    amendment to the registration. Name data on file will be used for generating the statement.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="500"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="IDNumber">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Filer ID number of the filer.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="15"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="IsAmendment" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>
                    If this is an amendment to the report, mark as true. If true, an explanation must be provided.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="AmendmentExplanation">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide an explanation of why the report is being amended.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="4000"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="unbounded" minOccurs="1" name="Section1">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              Use this section to identify the spokespersons and the ballot measures they are supporting/opposing.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="TransactionID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Paid Spokesperson transaction uploaded into CAL-ACCESS.
                    The TransactionID must match exactly the TransactionID assigned to the corresponding expenditure
                    containing the payee details reported on your next 450, 460, or 461.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Date" type="dateOrDateTime">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the date of the transaction. Must not be a  future date or an  error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="Spokesperson.CARS_Id">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and PrincipalOfficer transaction uploaded into CARS.
                    This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Spokesperson.FirstName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    First Name for the spokesperson.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SpokespersonAddress.Country">
                                <xs:annotation>
                                    <xs:documentation>
                    Country of address. US Addresses should be United States of America.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="60"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SpokespersonAddress.Street">
                                <xs:annotation>
                                    <xs:documentation>
                    First lineof address.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SpokespersonAddress.City">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with a City or equivalent address element.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SpokespersonAddress.State">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with a State or equivalent (Province) address element.
                    For US Addresses, use the standard 2 letter USPS abbreviation.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="10"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SpokespersonAddress.ZipCode" type="zipcodeFormat">
                                <xs:annotation>
                                    <xs:documentation>
                    Required for US addresses and any foreign addresses with an equivalent address element (Post Code, Postal Code, etc).
                    For US addresses, supply the ZipCode or Zip+5. For foreign addresses format as per the postal standards for the applicable country.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Occupation">
                                <xs:annotation>
                                    <xs:documentation>
                    The occupation of a spokesperson is required when an expenditure of any amount is paid to an individual for his or
                    her appearance in an advertisement that supports or opposes the qualification, passage, or defeat of a state or local
                    ballot measure and the advertisement states or suggests that the individual is a member of an occupation that requires
                    licensure, certification, or other specialized, documented training as a prerequisite to engage in that occupation.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="250"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="BallotMeasure.FilerID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and PrincipalOfficer transaction uploaded into CARS.
                    This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="NameOfBallotMeasure">
                                <xs:annotation>
                                    <xs:documentation>
                    Title/name of the Ballot Measure supported/opposed. An error will be raised if no candidate, committee or ballot
                    measure is  named.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="200"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="BallotNumberOrLetter">
                                <xs:annotation>
                                    <xs:documentation>
                    Number or ID assigned to the Ballot Question
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="SupportOppose" type="supportOpposeEnumeration">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Position taken regarding the candidate, officeholder, or measure. Must be either &quot;Support&quot; or &quot;Oppose&quot;.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Amount" type="xs:decimal">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the total amount of the expenditure.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="1" name="Verification">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              This section contains the signature attesting that the information contained in this filing is complete and accurate. The person signing this must be listed
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="TransactionID">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and PrincipalOfficer transaction uploaded into CARS.
                    This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="CARS_Id">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    TransactionID is the unique identifier and reference number for each Treasurer and PrincipalOfficer transaction uploaded into CARS.
                    This will be used as the identifier for the officers in subsequent amendments.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="ExecutedOn" type="dateOrDateTime">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Insert the date on which the signature was obtained. No future dates are allowed.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="NameOfResponsibleOfficer.FirstName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    First name of Responsible Officer. Must match the name of an active Responsible Officer linked to the
                    filer or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="50"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="NameOfResponsibleOfficer.LastName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Last name (surname) of Responsible Officer. Must match the name of an active Responsible Officer linked
                    to the filer or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="NameOfResponsibleOfficer.Title">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Include the title of the signatory. Must match the title of an active Responsible Officer linked to
                    the filer or an error will be raised.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="([\s\S]*[^ ][\s\S]*)?"/>
                                        <xs:maxLength value="100"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- Form Definitions - Lobbying -->
    <xs:element name="Form601">
        <xs:annotation>
            <xs:documentation xml:lang="en">
        Form 601
      </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="Header">
                    <xs:annotation>
                        <xs:documentation>
              This section is used to validate that the uploaded file conforms to the expected format, was created by certified software, and is being uploaded to the correct filer.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element default="1.07" maxOccurs="1" minOccurs="1" name="CARS_Ver" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the version of the XSD file against which this file was validated. This will be used to determine if the XSD file used is still supported.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element fixed="601" maxOccurs="1" minOccurs="1" name="CARS_Form" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the CAL-ACCESS form being submitted.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Soft_Name" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the Name of the Vendor of the software used to generate this file. Together with the Software Version, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Soft_Ver" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the version of the software used to generate this file. Together with the Software Name, this will be validated against the certified software provider list to ensure that the software used has been certified to use the API to upload filings.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="FilerId" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the ID Number assigned to this Lobbying Filing Entity. This will be validated against the database to ensure the file is loaded to the correct filer.
                  </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="1" name="Part0">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">
              Use this section to identify the Lobbying Firm and Legislative session for this registration.
            </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="LegislativeSessionName">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the LegislativeSessionName.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="500"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Filer.FilerId">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Provide the Filer ID of the Lobbying Firm. An error will be raised if this does not math the Filer ID of the filer.
                  </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:pattern value="[\s\S]*[^ ][\s\S]*"/>
                                        <xs:maxLength value="15"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="NameOfLobbyingFirm">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                    Enter the name of the Lobbying Firm. If this differs from that in the system for the ID number provided,
                    the system will change the name to match what is in the file.
                  </xs:documentation>
